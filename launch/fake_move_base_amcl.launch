<launch>

  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen" clear_params="true">
    <rosparam file="$(find bebop_control)/config/fake/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find bebop_control)/config/fake/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find bebop_control)/config/fake/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find bebop_control)/config/fake/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find bebop_control)/config/fake/base_local_planner_params.yaml" command="load" />
    <remap from="cmd_vel" to="bebop/cmd_vel" />
  </node>
  
</launch>
